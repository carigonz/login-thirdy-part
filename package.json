{
  "name": "login-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "homepage": "/login",
  "proxy": "http://localhost:5000/api/v0",
  "dependencies": {
    "@material-ui/core": "latest",
    "@material-ui/icons": "^4.11.2",
    "@types/express": "^4.17.9",
    "@types/gapi.auth2": "0.0.55",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "cors": "^2.8.5",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "material-ui-icons": "^1.0.0-beta.36",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-scripts": "latest",
    "ts-node-dev": "^1.1.1",
    "ts-react-google-login-component": "^1.1.9",
    "typescript": "latest"
  },
  "scripts": {
    "start": "run-p --race start:server start:app",
